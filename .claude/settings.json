{
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(curl:*)",
      "Bash(mkdir:*)",
      "Bash(find:*)",
      "Bash(ls:*)",
      "Bash(cp:*)"
    ],
    "deny": []
  },
  "hooks": {
    "PreCompact": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "python3 .claude/hooks/log_hook_event.py"
          },
          {
            "type": "command",
            "command": "afplay -v 0.7 .claude/sounds/precompact.mp3"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "python3 .claude/hooks/pre_tool_use.py"
          },
          {
            "type": "command",
            "command": "python3 .claude/hooks/log_hook_event.py"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "python3 .claude/hooks/post_tool_use.py"
          },
          {
            "type": "command",
            "command": "python3 .claude/hooks/log_hook_event.py"
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "python3 .claude/hooks/log_hook_event.py"
          },
          {
            "type": "command",
            "command": "afplay -v 0.7 .claude/sounds/session-start.mp3"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "python3 .claude/hooks/log_hook_event.py"
          },
          {
            "type": "command",
            "command": "afplay -v 0.7 .claude/sounds/stop.mp3"
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "afplay -v 0.7 .claude/sounds/subagent-stop.mp3"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "python3 .claude/hooks/user_prompt_submit.py"
          },
          {
            "type": "command",
            "command": "python3 .claude/hooks/log_hook_event.py"
          }
        ]
      }
    ]
  }
}
